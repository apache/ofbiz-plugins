<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>HeadlessCommerce Services</description>
    <vendor></vendor>
    <version>1.0</version>

    <service name="getPasswordHint" engine="java"
            location="org.apache.ofbiz.hc.api.customer.CustomerServices" invoke="getPasswordHint">
        <attribute name="username" mode="IN" type="String"/>
        <attribute name="passwordHint" mode="OUT" type="String" optional="true"/>
    </service>
    <service name="sendResetPasswordEmail" engine="java"
            location="org.apache.ofbiz.hc.api.customer.CustomerServices" invoke="sendResetPasswordEmail">
        <attribute name="username" mode="IN" type="String"/>
        <attribute name="webSiteId" mode="IN" type="String"/>
    </service>
    <service name="changePassword" engine="java"
            location="org.apache.ofbiz.hc.api.customer.CustomerServices" invoke="changePassword">
        <attribute name="customerPartyId" mode="IN" type="String"/>
        <attribute name="currentPassword" mode="IN" type="String"/>
        <attribute name="newPassword" mode="IN" type="String"/>
        <attribute name="newPasswordVerify" mode="IN" type="String"/>
        <attribute name="passwordHint" mode="IN" type="String" optional="true"/>
        <attribute name="requirePasswordChange" mode="IN" type="String" optional="true" default="N"/>
    </service>
    <service name="createCustomer" engine="java"
            location="org.apache.ofbiz.hc.api.customer.CustomerServices" invoke="createCustomer">
        <attribute name="webSiteId" mode="IN" type="String"/>
        <attribute name="personalTitle" mode="IN" type="String" optional="true"/>
        <attribute name="firstName" mode="IN" type="String"/>
        <attribute name="middleName" mode="IN" type="String" optional="true"/>
        <attribute name="lastName" mode="IN" type="String"/>
        <attribute name="suffix" mode="IN" type="String" optional="true"/>
        <attribute name="emailAddress" mode="IN" type="String"/>
        <attribute name="emailSolicitation" mode="IN" type="String" optional="true"/>
        <attribute name="username" mode="IN" type="String"/>
        <attribute name="password" mode="IN" type="String"/>
        <attribute name="confirmPassword" mode="IN" type="String"/>
        <attribute name="passwordHint" mode="IN" type="String" optional="true"/>
        <attribute name="shippingAddress" mode="IN" type="Map" optional="true">
            <attribute name="address1" mode="IN" type="String"/>
            <attribute name="address2" mode="IN" type="String" optional="true"/>
            <attribute name="city" mode="IN" type="String"/>
            <attribute name="postalCode" mode="IN" type="String"/>
            <attribute name="state" mode="IN" type="String" optional="true"/>
            <attribute name="country" mode="IN" type="String"/>
            <attribute name="allowSolicitation" mode="IN" type="String" optional="true"/>
        </attribute>
        <attribute name="homePhone" mode="IN" type="Map" optional="true">
            <attribute name="countryCode" mode="IN" type="String" optional="true"/>
            <attribute name="areaCode" mode="IN" type="String" optional="true"/>
            <attribute name="contactNumber" mode="IN" type="String"/>
            <attribute name="extension" mode="IN" type="String" optional="true"/>
            <attribute name="allowSolicitation" mode="IN" type="String" optional="true"/>
        </attribute>
        <attribute name="workPhone" mode="IN" type="Map" optional="true">
            <attribute name="countryCode" mode="IN" type="String" optional="true"/>
            <attribute name="areaCode" mode="IN" type="String" optional="true"/>
            <attribute name="contactNumber" mode="IN" type="String"/>
            <attribute name="extension" mode="IN" type="String" optional="true"/>
            <attribute name="allowSolicitation" mode="IN" type="String" optional="true"/>
        </attribute>
        <attribute name="faxNumber" mode="IN" type="Map" optional="true">
            <attribute name="countryCode" mode="IN" type="String" optional="true"/>
            <attribute name="areaCode" mode="IN" type="String" optional="true"/>
            <attribute name="contactNumber" mode="IN" type="String"/>
            <attribute name="allowSolicitation" mode="IN" type="String" optional="true"/>
        </attribute>
        <attribute name="mobilePhone" mode="IN" type="Map" optional="true">
            <attribute name="countryCode" mode="IN" type="String" optional="true"/>
            <attribute name="areaCode" mode="IN" type="String" optional="true"/>
            <attribute name="contactNumber" mode="IN" type="String"/>
            <attribute name="allowSolicitation" mode="IN" type="String" optional="true"/>
        </attribute>
    </service>
    <service name="validateCreateCustomer" engine="java"
            location="org.apache.ofbiz.hc.api.customer.CustomerServices" invoke="validateCreateCustomer">
        <implements service="createCustomer"/>
    </service>
    <service name="getCustomerProfile" engine="java"
            location="org.apache.ofbiz.hc.api.customer.CustomerServices" invoke="getCustomerProfile">
        <attribute name="customerPartyId" mode="INOUT" type="String"/>
        <attribute name="showOld" mode="IN" type="String" optional="true"/>
        <attribute name="filters" mode="INOUT" type="Map" optional="true"/>
        <attribute name="personalInformation" mode="OUT" type="Map" optional="false">
            <attribute name="personalTitle" mode="OUT" type="String" optional="true"/>
            <attribute name="firstName" mode="OUT" type="String"/>
            <attribute name="middleName" mode="OUT" type="String" optional="true"/>
            <attribute name="lastName" mode="OUT" type="String"/>
            <attribute name="suffix" mode="OUT" type="String" optional="true"/>
            <attribute name="userName" mode="OUT" type="String" optional="true"/>
        </attribute>
        <attribute name="contactMechs" mode="OUT" type="List" optional="true">
            <attribute name="contactMech" mode="OUT" type="Map" optional="true">
                <attribute name="contactMechId" mode="OUT" type="String"/>
                <attribute name="address1" mode="OUT" type="String"/>
                <attribute name="address2" mode="OUT" type="String" optional="true"/>
                <attribute name="city" mode="OUT" type="String"/>
                <attribute name="postalCode" mode="OUT" type="String"/>
                <attribute name="countryCode" mode="OUT" type="String" optional="true"/>
                <attribute name="areaCode" mode="OUT" type="String" optional="true"/>
                <attribute name="contactNumber" mode="OUT" type="String"/>
                <attribute name="extension" mode="OUT" type="String" optional="true"/>
                <attribute name="infoString" mode="OUT" type="String" optional="true"/>
                <attribute name="allowSolicitation" mode="OUT" type="String" optional="true"/>
                <attribute name="isDefault" mode="OUT" type="String" optional="true"/>
                <attribute name="state" mode="OUT" type="Map" optional="true">
                    <attribute name="geoId" mode="OUT" type="String" optional="true"/>
                    <attribute name="geoCode" mode="OUT" type="String" optional="true"/>
                    <attribute name="geoName" mode="OUT" type="String" optional="true"/>
                </attribute>
                <attribute name="country" mode="OUT" type="Map">
                    <attribute name="geoId" mode="OUT" type="String" optional="true"/>
                    <attribute name="geoCode" mode="OUT" type="String" optional="true"/>
                    <attribute name="geoName" mode="OUT" type="String" optional="true"/>
                </attribute>
                <attribute name="contactMechType" mode="OUT" type="Map" optional="true">
                    <attribute name="contactMechTypeId" mode="OUT" type="String" optional="true"/>
                    <attribute name="description" mode="OUT" type="String" optional="true"/>
                </attribute>
                <attribute name="contactMechPurposes" mode="OUT" type="List">
                    <attribute name="contactMechPurpose" mode="OUT" type="Map" optional="true">
                        <attribute name="contactMechPurposeTypeId" mode="OUT" type="String" optional="true"/>
                        <attribute name="description" mode="OUT" type="String" optional="true"/>
                    </attribute>
                </attribute>
             </attribute>
        </attribute>
        <attribute name="loyaltyPoints" mode="OUT" type="Map" optional="true">
            <attribute name="points" mode="OUT" type="Integer" optional="true"/>
            <attribute name="totalOrders" mode="OUT" type="Integer" optional="true"/>
            <attribute name="monthsToInclude" mode="OUT" type="Integer" optional="true"/>
        </attribute>
        <attribute name="paymentMethods" mode="OUT" type="List" optional="true">
            <attribute name="paymentMethod" mode="OUT" type="Map" optional="true">
                <attribute name="paymentMethodId" mode="OUT" type="String" optional="true"/>
                <attribute name="description" mode="OUT" type="String" optional="true"/>
                <attribute name="fromDate" mode="OUT" type="String" optional="true"/>
                <attribute name="thruDate" mode="OUT" type="String" optional="true"/>
                <!--credit card and gift card fields-->
                <attribute name="companyNameOnCard" mode="OUT" type="String" optional="true"/>
                <attribute name="titleOnCard" mode="OUT" type="String" optional="true"/>
                <attribute name="firstNameOnCard" mode="OUT" type="String" optional="true"/>
                <attribute name="middleNameOnCard" mode="OUT" type="String" optional="true"/>
                <attribute name="lastNameOnCard" mode="OUT" type="String" optional="true"/>
                <attribute name="suffixOnCard" mode="OUT" type="String" optional="true"/>
                <attribute name="cardNumber" mode="OUT" type="String" optional="true"/>
                <attribute name="expireDate" mode="OUT" type="String" optional="true"/>
                <!--EFT account field-->
                <attribute name="bankName" mode="OUT" type="String" optional="true"/>
                <attribute name="routingNumber" mode="OUT" type="String" optional="true"/>
                <attribute name="accountType" mode="OUT" type="String" optional="true"/>
                <attribute name="accountNumber" mode="OUT" type="String" optional="true"/>
                <attribute name="nameOnAccount" mode="OUT" type="String" optional="true"/>
                <attribute name="companyNameOnAccount" mode="OUT" type="String" optional="true"/>
                <attribute name="paymentMethodType" mode="OUT" type="Map" optional="true">
                    <attribute name="paymentMethodTypeId" mode="OUT" type="String" optional="true"/>
                    <attribute name="description" mode="OUT" type="String" optional="true"/>
                </attribute>
            </attribute>
        </attribute>
        <attribute name="contactLists" mode="OUT" type="List" optional="true">
            <attribute name="contactList" mode="OUT" type="Map" optional="true">
                <attribute name="contactListId" mode="OUT" type="String" optional="true"/>
                <attribute name="contactListName" mode="OUT" type="String" optional="true"/>
                <attribute name="description" mode="OUT" type="String" optional="true"/>
                <attribute name="fromDate" mode="OUT" type="String" optional="true"/>
                <attribute name="thruDate" mode="OUT" type="String" optional="true"/>
                <attribute name="email" mode="OUT" type="String" optional="true"/>
                <attribute name="status" mode="OUT" type="Map" optional="true">
                    <attribute name="statusId" mode="OUT" type="String" optional="true"/>
                    <attribute name="statusCode" mode="OUT" type="String" optional="true"/>
                    <attribute name="description" mode="OUT" type="String" optional="true"/>
                </attribute>
            </attribute>
        </attribute>
        <attribute name="communications" mode="OUT" type="List" optional="true">
            <attribute name="communication" mode="OUT" type="Map" optional="true">
                <attribute name="communicationEventId" mode="OUT" type="String" optional="true"/>
                <attribute name="fromPartyName" mode="OUT" type="String" optional="true"/>
                <attribute name="toPartyName" mode="OUT" type="String" optional="true"/>
                <attribute name="subject" mode="OUT" type="String" optional="true"/>
                <attribute name="entryDate" mode="OUT" type="String" optional="true"/>
                <attribute name="content" mode="OUT" type="String" optional="true"/>
            </attribute>
        </attribute>
    </service>
    <service name="getPartyPaymentMethods" engine="java"
            location="org.apache.ofbiz.hc.api.customer.CustomerServices" invoke="getPartyPaymentMethods">
        <attribute name="customerPartyId" mode="IN" type="String"/>
        <attribute name="showOld" mode="IN" type="String" optional="true"/>
        <attribute name="paymentMethods" mode="OUT" type="List" optional="true"/>
    </service>
    <service name="createCustomerPostalAddress" engine="java"
            location="org.apache.ofbiz.hc.api.customer.CustomerServices" invoke="createUpdateCustomerPostalAddress">
        <description>To create the postal address with purpose</description>
        <attribute name="customerPartyId" mode="IN" type="String"/>
        <attribute name="toName" mode="IN" type="String" optional="true"/>
        <attribute name="address1" mode="IN" type="String"/>
        <attribute name="address2" mode="IN" type="String" optional="true"/>
        <attribute name="city" mode="IN" type="String"/>
        <attribute name="postalCode" mode="IN" type="String"/>
        <attribute name="stateCode" mode="IN" type="String" optional="true"/>
        <attribute name="countryCode" mode="IN" type="String"/>
        <attribute name="allowSolicitation" mode="IN" type="String" optional="true"/>
        <attribute name="isDefault" mode="IN" type="String" optional="true"/>
        <attribute name="contactMechPurposeTypeId" mode="IN" type="String" optional="true"/>
    </service>
    <service name="updateCustomerPostalAddress" engine="java"
            location="org.apache.ofbiz.hc.api.customer.CustomerServices" invoke="createUpdateCustomerPostalAddress">
        <description>To update the postal address and to add a purpose</description>
        <attribute name="customerPartyId" mode="IN" type="String"/>
        <attribute name="contactMechId" mode="IN" type="String"/>
        <attribute name="toName" mode="IN" type="String" optional="true"/>
        <attribute name="address1" mode="IN" type="String" optional="true"/>
        <attribute name="address2" mode="IN" type="String" optional="true"/>
        <attribute name="city" mode="IN" type="String" optional="true"/>
        <attribute name="postalCode" mode="IN" type="String" optional="true"/>
        <attribute name="stateCode" mode="IN" type="String" optional="true"/>
        <attribute name="countryCode" mode="IN" type="String" optional="true"/>
        <attribute name="allowSolicitation" mode="IN" type="String" optional="true"/>
        <attribute name="isDefault" mode="IN" type="String" optional="true"/>
        <attribute name="contactMechPurposeTypeId" mode="IN" type="String" optional="true"/>
    </service>
    <service name="removeCustomerContactMech" engine="java"
            location="org.apache.ofbiz.hc.api.customer.CustomerServices" invoke="removeCustomerContactMechAndPurpose">
        <description>To remove the the party contact mech</description>
        <attribute name="customerPartyId" mode="IN" type="String"/>
        <attribute name="contactMechId" mode="IN" type="String"/>
    </service>
    <service name="removeCustomerContactMechPurpose" engine="java"
            location="org.apache.ofbiz.hc.api.customer.CustomerServices" invoke="removeCustomerContactMechAndPurpose">
        <description>To remove the the party contact mech purpose</description>
        <attribute name="customerPartyId" mode="IN" type="String"/>
        <attribute name="contactMechId" mode="IN" type="String"/>
        <attribute name="contactMechPurposeTypeId" mode="IN" type="String"/>
    </service>
    <service name="createCustomerTelecomNumber" engine="java"
            location="org.apache.ofbiz.hc.api.customer.CustomerServices" invoke="createUpdateCustomerTelecomNumber">
        <description>To create the telecom number with purpose</description>
        <attribute name="customerPartyId" mode="IN" type="String"/>
        <attribute name="countryCode" mode="IN" type="String" optional="true"/>
        <attribute name="areaCode" mode="IN" type="String" optional="true"/>
        <attribute name="contactNumber" mode="IN" type="String"/>
        <attribute name="extension" mode="IN" type="String" optional="true"/>
        <attribute name="allowSolicitation" mode="IN" type="String" optional="true"/>
        <attribute name="isDefault" mode="IN" type="String" optional="true"/>
        <attribute name="contactMechPurposeTypeId" mode="IN" type="String" optional="true"/>
    </service>
    <service name="updateCustomerTelecomNumber" engine="java"
            location="org.apache.ofbiz.hc.api.customer.CustomerServices" invoke="createUpdateCustomerTelecomNumber">
        <description>To update the telecom number and to add a purpose</description>
        <attribute name="customerPartyId" mode="IN" type="String"/>
        <attribute name="contactMechId" mode="IN" type="String"/>
        <attribute name="countryCode" mode="IN" type="String" optional="true"/>
        <attribute name="areaCode" mode="IN" type="String" optional="true"/>
        <attribute name="contactNumber" mode="IN" type="String" optional="true"/>
        <attribute name="extension" mode="IN" type="String" optional="true"/>
        <attribute name="allowSolicitation" mode="IN" type="String" optional="true"/>
        <attribute name="isDefault" mode="IN" type="String" optional="true"/>
        <attribute name="contactMechPurposeTypeId" mode="IN" type="String" optional="true"/>
    </service>
    <service name="createCustomerEmailAddress" engine="java"
            location="org.apache.ofbiz.hc.api.customer.CustomerServices" invoke="createUpdateCustomerEmailAddress">
        <description>To create the telecom number</description>
        <attribute name="customerPartyId" mode="IN" type="String"/>
        <attribute name="emailAddress" mode="IN" type="String"/>
        <attribute name="allowSolicitation" mode="IN" type="String" optional="true"/>
        <attribute name="contactMechPurposeTypeId" mode="IN" type="String" optional="true"/>
    </service>
    <service name="updateCustomerEmailAddress" engine="java"
            location="org.apache.ofbiz.hc.api.customer.CustomerServices" invoke="createUpdateCustomerEmailAddress">
        <description>To update the email address</description>
        <attribute name="customerPartyId" mode="IN" type="String"/>
        <attribute name="contactMechId" mode="IN" type="String"/>
        <attribute name="emailAddress" mode="IN" type="String" optional="true"/>
        <attribute name="allowSolicitation" mode="IN" type="String" optional="true"/>
        <attribute name="contactMechPurposeTypeId" mode="IN" type="String" optional="true"/>
    </service>
    <service name="createCustomerContactMech" engine="java"
            location="org.apache.ofbiz.hc.api.customer.CustomerServices" invoke="createUpdateCustomerContactMech">
        <description>To create contact mech</description>
        <attribute name="customerPartyId" mode="IN" type="String"/>
        <attribute name="contactMechTypeId" mode="IN" type="String"/>
        <attribute name="infoString" mode="IN" type="String"/>
        <attribute name="allowSolicitation" mode="IN" type="String" optional="true"/>
    </service>
    <service name="updateCustomerContactMech" engine="java"
            location="org.apache.ofbiz.hc.api.customer.CustomerServices" invoke="createUpdateCustomerContactMech">
        <description>To update contact mech</description>
        <attribute name="customerPartyId" mode="IN" type="String"/>
        <attribute name="contactMechTypeId" mode="IN" type="String"/>
        <attribute name="contactMechId" mode="IN" type="String"/>
        <attribute name="infoString" mode="IN" type="String" optional="true"/>
        <attribute name="allowSolicitation" mode="IN" type="String" optional="true"/>
    </service>
    <service name="createCustomerCreditCard" engine="java"
            location="org.apache.ofbiz.hc.api.customer.CustomerServices" invoke="createUpdateCustomerPaymentMethod">
        <description>creates credit card</description>
        <attribute name="customerPartyId" mode="IN" type="String"/>
        <attribute name="address" mode="IN" type="Map" optional="true">
            <attribute name="contactMechId" mode="IN" type="String" optional="true"/>
            <attribute name="address1" mode="IN" type="String" optional="true"/>
            <attribute name="address2" mode="IN" type="String" optional="true"/>
            <attribute name="city" mode="IN" type="String" optional="true"/>
            <attribute name="postalCode" mode="IN" type="String" optional="true"/>
            <attribute name="allowSolicitation" mode="IN" type="String" optional="true"/>
            <attribute name="stateCode" mode="IN" type="String" optional="true"/>
            <attribute name="countryCode" mode="IN" type="String" optional="true"/>
        </attribute>
        <attribute name="creditCard" mode="IN" type="Map" optional="true">
            <attribute name="companyNameOnCard" mode="IN" type="String" optional="true"/>
            <attribute name="titleOnCard" mode="IN" type="String" optional="false"/>
            <attribute name="firstNameOnCard" mode="IN" type="String"/>
            <attribute name="middleNameOnCard" mode="IN" type="String" optional="true"/>
            <attribute name="lastNameOnCard" mode="IN" type="String"/>
            <attribute name="suffixOnCard" mode="IN" type="String" optional="true"/>
            <attribute name="cardNumber" mode="IN" type="String"/>
            <attribute name="expireDate" mode="IN" type="String"/>
        </attribute>
    </service>
    <service name="updateCustomerCreditCard" engine="java"
            location="org.apache.ofbiz.hc.api.customer.CustomerServices" invoke="createUpdateCustomerPaymentMethod">
        <description>updates credit card</description>
        <attribute name="customerPartyId" mode="IN" type="String"/>
        <attribute name="paymentMethodId" mode="IN" type="String"/>
        <attribute name="address" mode="IN" type="Map" optional="false">
            <attribute name="contactMechId" mode="IN" type="String" optional="true"/>
            <attribute name="address1" mode="IN" type="String" optional="true"/>
            <attribute name="address2" mode="IN" type="String" optional="true"/>
            <attribute name="city" mode="IN" type="String" optional="true"/>
            <attribute name="postalCode" mode="IN" type="String" optional="true"/>
            <attribute name="allowSolicitation" mode="IN" type="String" optional="true"/>
            <attribute name="stateCode" mode="IN" type="String" optional="true"/>
            <attribute name="countryCode" mode="IN" type="String" optional="true"/>
        </attribute>
        <attribute name="creditCard" mode="IN" type="Map" optional="true">
            <attribute name="companyNameOnCard" mode="IN" type="String" optional="true"/>
            <attribute name="titleOnCard" mode="IN" type="String" optional="false"/>
            <attribute name="firstNameOnCard" mode="IN" type="String"/>
            <attribute name="middleNameOnCard" mode="IN" type="String" optional="true"/>
            <attribute name="lastNameOnCard" mode="IN" type="String"/>
            <attribute name="suffixOnCard" mode="IN" type="String" optional="true"/>
            <attribute name="cardNumber" mode="IN" type="String"/>
            <attribute name="expireDate" mode="IN" type="String"/>
        </attribute>
    </service>
    <service name="createCustomerGiftCard" engine="java"
            location="org.apache.ofbiz.hc.api.customer.CustomerServices" invoke="createUpdateCustomerPaymentMethod">
        <description>creates gift card</description>
        <attribute name="customerPartyId" mode="IN" type="String"/>
        <attribute name="giftCard" mode="IN" type="Map" optional="true">
            <attribute name="cardNumber" mode="IN" type="String"/>
            <attribute name="expireDate" mode="IN" type="String"/>
        </attribute>
    </service>
    <service name="updateCustomerGiftCard" engine="java"
            location="org.apache.ofbiz.hc.api.customer.CustomerServices" invoke="createUpdateCustomerPaymentMethod">
        <description>updates gift card</description>
        <attribute name="customerPartyId" mode="IN" type="String"/>
        <attribute name="paymentMethodId" mode="IN" type="String"/>
        <attribute name="giftCard" mode="IN" type="Map" optional="true">
            <attribute name="cardNumber" mode="IN" type="String"/>
            <attribute name="expireDate" mode="IN" type="String"/>
        </attribute>
    </service>
    <service name="createCustomerEftAccount" engine="java"
            location="org.apache.ofbiz.hc.api.customer.CustomerServices" invoke="createUpdateCustomerPaymentMethod">
        <description>creates EFT account</description>
        <attribute name="customerPartyId" mode="IN" type="String"/>
        <attribute name="eftAccount" mode="IN" type="Map" optional="true">
            <attribute name="bankName" mode="IN" type="String"/>
            <attribute name="routingNumber" mode="IN" type="String"/>
            <attribute name="accountType" mode="IN" type="String"/>
            <attribute name="accountNumber" mode="IN" type="String"/>
            <attribute name="nameOnAccount" mode="IN" type="String"/>
            <attribute name="companyNameOnAccount" mode="IN" type="String"/>
        </attribute>
    </service>
    <service name="updateCustomerEftAccount" engine="java"
            location="org.apache.ofbiz.hc.api.customer.CustomerServices" invoke="createUpdateCustomerPaymentMethod">
        <description>updates EFT account</description>
        <attribute name="customerPartyId" mode="IN" type="String"/>
        <attribute name="paymentMethodId" mode="IN" type="String"/>
        <attribute name="eftAccount" mode="IN" type="Map" optional="true">
            <attribute name="bankName" mode="IN" type="String"/>
            <attribute name="routingNumber" mode="IN" type="String"/>
            <attribute name="accountType" mode="IN" type="String"/>
            <attribute name="accountNumber" mode="IN" type="String"/>
            <attribute name="nameOnAccount" mode="IN" type="String"/>
            <attribute name="companyNameOnAccount" mode="IN" type="String"/>
        </attribute>
    </service>
</services>