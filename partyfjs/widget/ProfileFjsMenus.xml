            <condition>
              <or>
                  <if-empty field="PartyAttributesMenu"/>
                  <if-compare field="PartyAttributesMenu" operator="equals" value="Y"/>
              </or>
            </condition>
            <link text="${uiLabelMap.PartyAttributes}" target="showPortletFj" link-type="set-area" url-mode="intra-post" target-window="${areaId}">
              <parameter param-name="partyId" from-field="parameters.partyId"/>
                <parameter param-name="portalPageId" from-field="parameters.portalPageId"/>
                <parameter param-name="portalPortletId" value="PartyDetail"/>
                <parameter param-name="portletSeqId"    value="00001"/>
                <parameter param-name="subPortletId" value="PartyAttributes"/>
                <image src="${iconsPurpose.Attributes}" title="${uiLabelMap.PartyAttributes}"/>
            </link>
        </menu-item>
    </menu>
    <menu name="PartyExternalMenu" menu-container-style="button-bar button-style-2">
