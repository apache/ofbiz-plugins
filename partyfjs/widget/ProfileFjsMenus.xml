    <menu name="PartyRelationships" extends="CommonTabBarMenu" extends-resource="component://common/widget/CommonMenus.xml">
      <menu-item name="AddPartyRelationshipFrom">
          <condition>
            <and>
              <if-compare field="showEditButton" operator="equals" value="Y"/>
