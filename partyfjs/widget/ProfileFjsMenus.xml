              <parameter param-name="showHistory" value="N"/>
              <parameter param-name="roleId" from-field="parameters.roleId"/>
              <auto-parameters-portlet/>
              <image src="${iconsPurpose.HideHistory}" title="${uiLabelMap.IconsTooltips_HideHistory}"/>
          </link>
      </menu-item>
    </menu>

